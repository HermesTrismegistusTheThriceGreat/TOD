# Get NeonDB connection string from: https://console.neon.tech
DATABASE_URL=postgresql://username:password@host/database?sslmode=require

NEON_DB_API_KEY=your_neon_api_key_here

# Alpaca Trading API
# Get your API keys from: https://alpaca.markets/
# For paper trading, use your paper trading keys from: https://app.alpaca.markets/paper/dashboard/overview
# Leave as placeholder values to disable Alpaca features
ALPACA_API_KEY=your_alpaca_api_key_here
ALPACA_SECRET_KEY=your_alpaca_secret_key_here
ALPACA_PAPER=true
ALPACA_DATA_FEED=iex

# Encryption Configuration
# Generate key with: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"
# IMPORTANT: Keep this key secret and never commit it to version control
ENCRYPTION_KEY=

# Frontend and Backend Configuration
VITE_BACKEND_URL=http://127.0.0.1:9403
FRONTEND_PORT=5175
BACKEND_PORT=9403

# Model Configuration
ORCHESTRATOR_MODEL=claude-sonnet-4-5-20250929
